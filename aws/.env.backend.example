# AWS Backend Environment Variables
# Copy this file to .env and fill in your values

# ===========================================
# RDS PostgreSQL
# ===========================================
DATABASE_URL=postgres://username:password@your-rds-endpoint.region.rds.amazonaws.com:5432/smartparking?sslmode=require

# ===========================================
# AWS S3
# ===========================================
USE_AWS_S3=true
AWS_REGION=ap-southeast-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
S3_BUCKET=smart-parking-images

# ===========================================
# Server
# ===========================================
PORT=8080
GIN_MODE=release

# ===========================================
# AI Service (Optional)
# ===========================================
AI_SERVICE_URL=http://localhost:5000
